  }
  else {
    for (double i = startAngle; i >= endAngle; i -= ARC_STEP) {
      goTo(radius * cos(i) + centerX, radius * sin(i) + centerY);
    }
  }
}

void elevatorRun() {
  while (abs(elevator.distanceToGo()) > 0) {
    elevator.run();
  }
  delay(500);
}

void goTo(int x, int y) {
  double angle1 = pgm_read_byte(&xA[x]);
  double angle2 = pgm_read_byte(&yA[y]);
  double joint1move = angle1 / ARM_STEPS;
  double joint2move = angle2 / (ARM_STEPS*JOINT_2_GEAR_REDUCTION);
  joint1.moveTo(joint1move);
  joint2.moveTo(joint2move);

  runLoop();
  currentX = x;
  currentY = y;
}

void runLoop() {
  while (abs(joint1.distanceToGo()) > 0 || abs(joint2.distanceToGo()) > 0 || abs(elevator.distanceToGo()) > 0) {
    joint1.run();
    joint2.run();
    elevator.run();
  }
}
